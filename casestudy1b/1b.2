    "import numpy as np\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "def todB(num):\n",
    "    return 10 * np.log10(num)\n",
    "\n",
    "def toLinear(num):\n",
    "    return 10**(num/10)"
   ]
  },
